{
  "name": "neutrino-electron-boilerplate",
  "description": "A boilerplate for the anti-boilerplate toolchain.",
  "version": "0.1.0",
  "license": "MIT",
  "author": {
    "name": "Jeff Friesen"
  },
  "scripts": {
    "start": "neutrino start",
    "build": "neutrino build",
    "test": "neutrino test",
    "coverage": "neutrino test --coverage",
    "gen": "plop --plopfile generators/plopfile.js"
  },
  "devDependencies": {
    "babel-plugin-tcomb": "^0.3.24",
    "enzyme": "^2.7.1",
    "eslint": "^3.17.1",
    "eslint-plugin-react": "^6.10.0",
    "inquirer-directory": "^2.1.0",
    "neutrino": "^4.3.1",
    "neutrino-preset-class-properties": "^0.1.0",
    "neutrino-preset-decorators": "^0.1.0",
    "neutrino-preset-jest": "^4.3.0",
    "neutrino-preset-lint-prettier": "git+https://github.com/jefffriesen/neutrino-preset-lint-prettier.git",
    "neutrino-preset-react": "^4.2.3",
    "plop": "^1.7.4",
    "react-addons-test-utils": "^15.4.2"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "lodash": "^4.17.4",
    "mobx": "^3.1.5",
    "mobx-react": "^4.1.2",
    "mobx-remotedev": "^0.2.6",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.8",
    "react-dom": "^15.4.2",
    "react-router-dom": "^4.0.0",
    "tcomb": "^3.2.20"
  },
  "config": {
    "devtool": "source-map",
    "presets": [
      "neutrino-preset-react",
      "neutrino-preset-class-properties",
      "neutrino-preset-decorators",
      "neutrino-custom-tcomb.js",
      "neutrino-preset-jest",
      "neutrino-custom-jest.js"
    ],
    "neutrino": {
      "entry": {
        "vendor": [
          "react",
          "react-dom",
          "lodash"
        ]
      }
    }
  }
}
