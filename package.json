{
  "name": "neutrino-electron-reference",
  "description": "A reference implementation for neutrino with Electron, React, linting, type checking & decorators",
  "version": "0.1.0",
  "license": "MIT",
  "author": {
    "name": "Jeff Friesen"
  },
  "main": "main.js",
  "scripts": {
    "start": "neutrino start",
    "start-electron": "neutrino build && electron .",
    "build": "neutrino build",
    "test": "neutrino test",
    "coverage": "neutrino test --coverage"
  },
  "devDependencies": {
    "babel-plugin-tcomb": "^0.3.24",
    "electron": "^1.6.2",
    "electron-builder": "^15.3.0",
    "enzyme": "^2.7.1",
    "neutrino": "^4.3.1",
    "neutrino-preset-airbnb-base": "^4.3.1",
    "neutrino-preset-class-properties": "^0.1.0",
    "neutrino-preset-decorators": "^0.1.0",
    "neutrino-preset-jest": "^4.3.0",
    "neutrino-preset-lint-prettier": "git+https://github.com/jefffriesen/neutrino-preset-lint-prettier.git",
    "neutrino-preset-react": "^4.2.3",
    "react-addons-test-utils": "^15.4.2"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "mobx": "^3.1.5",
    "mobx-react": "^4.1.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "tcomb": "^3.2.20"
  },
  "config": {
    "presets": [
      "neutrino-preset-react",
      "neutrino-preset-class-properties",
      "neutrino-preset-decorators",
      "neutrino-preset-jest",
      "neutrino-preset-lint-prettier",
      "neutrino-custom.js"
    ],
    "neutrino": {
      "entry": {
        "vendor": [
          "react",
          "react-dom",
          "lodash"
        ]
      }
    }
  }
}
